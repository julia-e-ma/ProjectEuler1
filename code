#set lists and variables
total=0
multiples=[]
# function to find multiples of three and five lower than int
def multiples_three_five(integer):
  for i in range(2,integer):
    if (i%3==0 or i%5==0):
      multiples.append(i)
  return multiples

multiples_1000=multiples_three_five(1000)
print(multiples_1000)
total=sum(multiples_1000)
print(total)
